{
  "RootUrl": "<root url here>",
  "AllowedHosts": "*",
  "Cache": {
    "MaxSizeBytes": 524288000,
    "DefaultDataCellSize": 16,
    "DefaultUpdateTaskSize": 50,
    "DefaultTableGroupSize": 100,
    "DefaultFileListSize": 350000,
    "DefaultMetaSize": 200000
  },
  "FeatureManagement": {
    "CacheController": true
  },
  "QueryLimits": {
    "JsonMaxCells": 100000,
    "JsonStatMaxCells": 50000
  },
  "Localization": {
    "DefaultLanguage": "fi",
    "SupportedLanguages": [
      "fi",
      "sv",
      "en"
    ]
  },
  "DataBases": [
    {
      "Type": "Mounted",
      "Id": "StatFin",
      "CacheConfig": {
        "TableList": {
          "SlidingExpirationSeconds": 30,
          "AbsoluteExpirationSeconds": 1440
        },
        "Meta": {
          "SlidingExpirationSeconds": 60,
          "AbsoluteExpirationSeconds": 4320
        },
        "Data": {
          "SlidingExpirationSeconds": 15,
          "AbsoluteExpirationSeconds": 1440
        },
        "RevalidationIntervalMs": 500,
        "Groupings": {
          "SlidingExpirationSeconds": 45,
          "AbsoluteExpirationSeconds": 4320
        }
      },
      "Custom": {
        "RootPath": "<test-db-path>"
      }
    }
  ],
  "OpenApi": {
    "ContactName": "<contact name>",
    "ContactUrl": "<url>",
    "ContactEmail": "<example@email.com>",
    "LicenseName": "<license>",
    "LicenseUrl": "<license_url>"
  },
  "LogOptions": {
    "Folder": "<folder>",
    "SysId": "PxApi",
    "Level": "Debug",
    "AuditLog": {
      "Enabled": true,
      "Headers": []
    }
  }
}
